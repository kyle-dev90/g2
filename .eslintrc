{
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true
    },
    "extends": [
      "react-app"
    ],
    "plugins": [
      "react"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ],
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      }
    },
    "rules": {
      "comma-dangle": "off",
      "no-console": "off"
    }
  }
  